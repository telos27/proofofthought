    {
      "name": "prevents_blocks_effect",
      "quantified_vars": [
        {"name": "X", "type": "Event"},
        {"name": "Y", "type": "Event"}
      ],
      "antecedent": {"predicate": "prevents", "arguments": ["X", "Y"]},
      "consequent": {"predicate": "causes", "arguments": ["X", "Y"], "negated": true},

  "version": "0.1",
  "description": "Basic commonsense knowledge about objects, properties, and causation",

  "facts": [],

  "rules": [
    {
      "name": "causes_implies_possible_effect",
      "quantified_vars": [
        {"name": "X", "type": "Event"},
        {"name": "Y", "type": "Event"}
      ],
